    "@types/hast": "^3.0.4",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/throttle-debounce": "^5.0.2",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "globals": "^16.3.0",
    "jsdom": "^27.0.1",
    "shadcn": "^3.4.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.53.0",
    "ws": "^8.18.0"
  }
}
