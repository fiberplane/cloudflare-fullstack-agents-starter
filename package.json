{
  "name": "@fiberplane/console",
  "description": "The Fiberplane Console",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome ci .",
    "format": "biome check --write .",
    "test": "vitest",
    "typecheck": "tsc -b",
    "preview": "bun run build && vite preview",
    "deploy": "bun run build && wrangler deploy --env=''",
    "deploy:preview": "CLOUDFLARE_ENV=preview bun run build && wrangler deploy --env=''",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "do-db:generate": "drizzle-kit generate --config drizzle-do.config.ts",
    "do-db:studio": "STUDIO_LOCAL_DB=true drizzle-kit studio --config drizzle-do.config.ts",
    "db:touch": "wrangler d1 execute DB --local --command='SELECT 1'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:migrate:prod": "ENVIRONMENT=production drizzle-kit migrate",
    "db:setup": "bun db:touch && bun db:generate && bun db:migrate",
    "db:studio": "drizzle-kit studio",
    "auth:generate": "bunx @better-auth/cli@latest generate --config better-auth.config.ts",
    "seed:playwright": "bun run scripts/seed-playwright-user.ts",
    "ci:db:generate": "bun run db:touch && bun run db:generate",
    "ci:deploy": "bun run build && cd dist/fp_cf_starter && bun wrangler deploy --env=''",
    "ci:deploy:preview": "CLOUDFLARE_ENV=preview bun run build && cd dist/fp_cf_starter && CLOUDFLARE_ENV=preview bun wrangler deploy --env=''",
    "ci:db:migrate:preview": "wrangler d1 migrations apply DB --remote --env preview",
    "ci:db:migrate:prod": "wrangler d1 migrations apply DB --remote"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.35",
    "@ai-sdk/openai": "^2.0.53",
    "@ai-sdk/react": "^2.0.76",
    "@hono/zod-validator": "^0.7.4",
    "@hookform/resolvers": "^5.2.2",
    "@logtape/logtape": "^1.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@shikijs/langs-precompiled": "^3.13.0",
    "@shikijs/rehype": "^3.13.0",
    "@tailwindcss/vite": "^4.1.15",
    "@tanstack/react-form": "^1.23.7",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.133.20",
    "@tanstack/react-router-devtools": "^1.133.20",
    "@xstate/react": "^6.0.0",
    "agents": "^0.2.26",
    "ai": "^5.0.103",
    "ai-gateway-provider": "^2.0.0",
    "better-auth": "^1.3.28",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "hast": "^1.0.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "hastscript": "^9.0.1",
    "hono": "^4.10.1",
    "lucide-react": "^0.546.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-markdown": "^10.1.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-linkify": "^1.1.0",
    "shiki": "^3.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.15",
    "throttle-debounce": "^5.0.2",
    "unified": "^11.0.5",
    "use-stick-to-bottom": "^1.1.1",
    "xstate": "^5.23.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
